{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "noImplicitAny": false,
    "paths": {
      "@definitions/*": ["src/definitions/*"],
      "@definitions": ["src/definitions"],
      "@interfaces/*": ["src/interfaces/*"],
      "@interfaces": ["src/interfaces"],
      "@components/*": ["src/components/*"],
      "@components": ["src/components"],
      "@styles/*": ["src/styles/*"],
      "@styles": ["src/styles"],
      "@public/*": ["public/*"],
      "@public": ["public"],
      "@redux/*": ["src/redux/*"],
      "@redux": ["src/redux"],
      "@saga/*": ["src/saga/*"],
      "@saga": ["src/saga"],
      "@apollo/*": ["src/apollo/*"],
      "@apollo": ["src/apollo"],
      "@ENV/*": ["src/assets/util/*"],
      "@ENV": ["src/assets/util"],
      "@axios/*": ["src/axios/*"],
      "@axios": ["src/axios"],
      "@screens/*": ["src/screens/*"],
      "@screens": ["src/screens"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
